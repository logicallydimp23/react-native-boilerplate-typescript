{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "pretty": false,
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "@helpers": [
        "src/helpers"
      ],
      "@base-components": [
        "src/components/base"
      ],
      "@custom-components": [
        "src/components/custom"
      ],
      "@screens": [
        "src/screens"
      ],
      "@navigationService": [
        "src/config/router/NavigationService"
      ],
      "@images": [
        "src/assets/images"
      ],
      "@themes": [
        "src/config/themes"
      ],
      "@translations": [
        "src/assets/translations"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@animation": [
        "src/assets/animations"
      ]
    }
  },
}